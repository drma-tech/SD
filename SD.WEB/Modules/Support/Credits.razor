@page "/support/credits"
@using SD.WEB.Modules.Provider.Resources
@using SD.WEB.Modules.Suggestions.Components
@inherits PageCore<Credits>

<SeoHeader Title="Credits" Url=@("/credits")></SeoHeader>

<Container>
    <HeadPage Title="TMDB" ImageSource="logo/tmdb.png" Text="@Translations.tmdb_desc" Link="https://www.themoviedb.org/"
              IsAuthenticated="IsAuthenticated" ShowShare="false">
    </HeadPage>

    <HeadPage Title="IMDB" ImageSource="logo/imdb.png" Text="@Translations.imdb_desc" Link="https://www.imdb.com/"
              IsAuthenticated="IsAuthenticated" ShowShare="false">
    </HeadPage>

    <HeadPage Title="JustWatch" ImageSource="logo/justwatch.png" Text="All your streaming services in one app." Link="https://www.justwatch.com/"
              IsAuthenticated="IsAuthenticated" ShowShare="false">
    </HeadPage>

    <HeadPage Title="Blazorise" ImageSource="logo/blazorise.png" Text="Blazorise is a component library built on top of Blazor with support for CSS frameworks like Bootstrap, Tailwind, Bulma, AntDesign and Material." Link="https://blazorise.com/"
              IsAuthenticated="IsAuthenticated" ShowShare="false">
    </HeadPage>
</Container>