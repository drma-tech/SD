{
  "routes": [
    {
      "route": "/api/public/*",
      "allowedRoles": [ "anonymous" ]
    },
    {
      "route": "/api/principal/remove",
      "allowedRoles": [ "anonymous" ]
    },
    {
      "route": "/api/adm/*",
      "allowedRoles": [ "administrator" ]
    },
    {
      "route": "/api/*",
      "allowedRoles": [ "authenticated" ]
    }
  ],
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": [
      "*.{css,scss,js,png,gif,ico,jpg,svg,ttf,woff2,webmanifest}"
    ]
  },
  "globalHeaders": {
    "content-security-policy": "default-src 'self'; frame-src 'self' ; script-src 'self' ; connect-src *; style-src * 'unsafe-inline'; img-src * data:; font-src * data:;",
    "Access-Control-Allow-Origin": "www.web-standards.com"
  }
}
